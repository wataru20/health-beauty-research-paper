        // ãƒãƒ£ãƒ¼ãƒˆä½œæˆé–¢æ•°
        function createResearchChart(topic) {
            const ctx = document.getElementById('researchTrendChart');
            if (!ctx) return;

            new Chart(ctx.getContext('2d'), {
                type: 'line',
                data: {
                    labels: ['2022', '2023 Q1', '2023 Q2', '2023 Q3', '2023 Q4', '2024'],
                    datasets: [{
                        label: 'ç ”ç©¶è«–æ–‡æ•°',
                        data: [2, 3, 4, 5, 8, 12],
                        borderColor: '#667eea',
                        backgroundColor: 'rgba(102, 126, 234, 0.1)',
                        tension: 0.4
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false
                }
            });
        }

        function createPredictionChart(topic) {
            const ctx = document.getElementById('predictionChart');
            if (!ctx) return;

            new Chart(ctx.getContext('2d'), {
                type: 'line',
                data: {
                    labels: ['ç¾åœ¨', '3ãƒ¶æœˆå¾Œ', '6ãƒ¶æœˆå¾Œ', '9ãƒ¶æœˆå¾Œ', '12ãƒ¶æœˆå¾Œ'],
                    datasets: [{
                        label: 'æˆé•·ç‡äºˆæ¸¬',
                        data: [topic.growth, topic.growth * 1.2, topic.growth * 1.5, topic.growth * 1.7, topic.growth * 2],
                        borderColor: '#764ba2',
                        backgroundColor: 'rgba(118, 75, 162, 0.1)',
                        tension: 0.4
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false
                }
            });
        }

        function createIngredientChart(ingredient) {
            const ctx = document.getElementById('ingredientTrendChart');
            if (!ctx) return;

            new Chart(ctx.getContext('2d'), {
                type: 'bar',
                data: {
                    labels: ['2019', '2020', '2021', '2022', '2023', '2024'],
                    datasets: [{
                        label: 'ç ”ç©¶è«–æ–‡æ•°',
                        data: ingredient.trends,
                        backgroundColor: 'rgba(102, 126, 234, 0.7)'
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false
                }
            });
        }

        function createIngredientPredictionChart(ingredient) {
            const ctx = document.getElementById('ingredientPredictionChart');
            if (!ctx) return;

            const lastValue = ingredient.trends[ingredient.trends.length - 1];
            new Chart(ctx.getContext('2d'), {
                type: 'line',
                data: {
                    labels: ['2024', '2025 Q1', '2025 Q2', '2025 Q3', '2025 Q4', '2026'],
                    datasets: [{
                        label: 'äºˆæ¸¬å€¤',
                        data: [lastValue, lastValue * 1.1, lastValue * 1.25, lastValue * 1.4, lastValue * 1.6, lastValue * 1.8],
                        borderColor: '#f5576c',
                        backgroundColor: 'rgba(245, 87, 108, 0.1)',
                        borderDash: [5, 5],
                        tension: 0.4
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false
                }
            });
        }

        // ãƒ¢ãƒ¼ãƒ€ãƒ«ã‚’é–‰ã˜ã‚‹
        function closeModal() {
            document.getElementById('detailModal').style.display = 'none';
        }

        // ESCã‚­ãƒ¼ã§ãƒ¢ãƒ¼ãƒ€ãƒ«ã‚’é–‰ã˜ã‚‹
        window.onclick = function(event) {
            const modal = document.getElementById('detailModal');
            if (event.target == modal) {
                modal.style.display = 'none';
            }
        }

        document.addEventListener('keydown', function(event) {
            if (event.key === 'Escape') {
                closeModal();
            }
        });

        // åˆæœŸåŒ–
        document.addEventListener('DOMContentLoaded', function() {
            displayHotTopics();
            displayIngredients();
        });

        // ãƒ‡ãƒ¼ã‚¿ã®è‡ªå‹•æ›´æ–°ã‚·ãƒŸãƒ¥ãƒ¬ãƒ¼ã‚·ãƒ§ãƒ³ï¼ˆå®Ÿéš›ã¯APIã‹ã‚‰å–å¾—ï¼‰
        setInterval(() => {
            // ãƒªã‚¢ãƒ«ã‚¿ã‚¤ãƒ ãƒ‡ãƒ¼ã‚¿æ›´æ–°ã®ã‚·ãƒŸãƒ¥ãƒ¬ãƒ¼ã‚·ãƒ§ãƒ³
            dataStore.hotTopics.forEach(topic => {
                topic.growth += Math.random() * 10 - 5; // -5 to +5
                topic.growth = Math.max(0, topic.growth);
            });

            // è¡¨ç¤ºã‚’æ›´æ–°ï¼ˆãƒ¢ãƒ¼ãƒ€ãƒ«ãŒé–‹ã„ã¦ã„ãªã„å ´åˆã®ã¿ï¼‰
            const modal = document.getElementById('detailModal');
            if (modal.style.display !== 'block') {
                displayHotTopics();
            }
        }, 30000); // 30ç§’ã”ã¨ã«æ›´æ–°
    </script>
</body>
</html>