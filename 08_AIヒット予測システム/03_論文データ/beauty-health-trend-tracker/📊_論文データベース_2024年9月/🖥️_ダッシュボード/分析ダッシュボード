                    clinicalTrials: 15,
                    commercialized: 10,
                    trends: [60, 75, 90, 110, 130, 143]
                }
            ]
        };

        // ç¾åœ¨é¸æŠã•ã‚Œã¦ã„ã‚‹æœŸé–“ã¨ãƒ©ãƒ³ã‚­ãƒ³ã‚°ç¨®åˆ¥
        let currentPeriod = '30d';
        let currentRanking = 'growth';
        let currentIngredientPeriod = '30d';

        // æœŸé–“ã‚’å¤‰æ›´
        function changePeriod(period) {
            currentPeriod = period;

            // ãƒœã‚¿ãƒ³ã®ã‚¢ã‚¯ãƒ†ã‚£ãƒ–çŠ¶æ…‹ã‚’æ›´æ–°
            document.querySelectorAll('.period-selector .period-btn').forEach(btn => {
                btn.classList.remove('active');
            });
            event.target.classList.add('active');

            // ãƒ›ãƒƒãƒˆãƒˆãƒ”ãƒƒã‚¯ã‚’å†è¡¨ç¤º
            displayHotTopics();
        }

        // ãƒ©ãƒ³ã‚­ãƒ³ã‚°ç¨®åˆ¥ã‚’å¤‰æ›´
        function changeRanking(rankingType) {
            currentRanking = rankingType;

            // ãƒœã‚¿ãƒ³ã®ã‚¢ã‚¯ãƒ†ã‚£ãƒ–çŠ¶æ…‹ã‚’æ›´æ–°
            document.querySelectorAll('.ranking-btn').forEach(btn => {
                if (rankingType === 'growth') {
                    if (btn.textContent === 'æˆé•·ç‡é †') {
                        btn.style.background = 'linear-gradient(90deg, #667eea 0%, #764ba2 100%)';
                        btn.style.color = 'white';
                    } else {
                        btn.style.background = '#f0f0f0';
                        btn.style.color = '#333';
                    }
                } else {
                    if (btn.textContent === 'è«–æ–‡æ•°é †') {
                        btn.style.background = 'linear-gradient(90deg, #667eea 0%, #764ba2 100%)';
                        btn.style.color = 'white';
                    } else {
                        btn.style.background = '#f0f0f0';
                        btn.style.color = '#333';
                    }
                }
            });

            // ãƒ›ãƒƒãƒˆãƒˆãƒ”ãƒƒã‚¯ã‚’å†è¡¨ç¤º
            displayHotTopics();
        }

        // ãƒ›ãƒƒãƒˆãƒˆãƒ”ãƒƒã‚¯ã‚¹ã‚’è¡¨ç¤º
        function displayHotTopics() {
            const container = document.getElementById('hotTopicsList');
            container.innerHTML = '';

            let topics = [...dataStore.hotTopicsByPeriod[currentPeriod]];

            // ãƒ©ãƒ³ã‚­ãƒ³ã‚°ç¨®åˆ¥ã«ã‚ˆã£ã¦ã‚½ãƒ¼ãƒˆ
            if (currentRanking === 'growth') {
                topics.sort((a, b) => b.growth - a.growth);
            } else {
                topics.sort((a, b) => b.papers - a.papers);
            }

            // ä¸Šä½5ä»¶ã®ã¿è¡¨ç¤º
            topics.slice(0, 5).forEach((topic, index) => {
                const li = document.createElement('li');
                li.className = 'hot-topic-item';
                li.onclick = () => showTopicDetail(topic);

                if (currentRanking === 'growth') {
                    li.innerHTML = `
                        <span class="topic-name">${index + 1}. ${topic.name}</span>
                        <span class="topic-growth">+${topic.growth}%</span>
                    `;
                } else {
                    li.innerHTML = `
                        <span class="topic-name">${index + 1}. ${topic.name}</span>
                        <span class="topic-growth">${topic.papers}ä»¶</span>
                    `;
                }
                container.appendChild(li);
            });
        }

        // æˆåˆ†ã®æœŸé–“ã‚’å¤‰æ›´
        function changeIngredientPeriod(period) {
            currentIngredientPeriod = period;

            // ãƒœã‚¿ãƒ³ã®ã‚¢ã‚¯ãƒ†ã‚£ãƒ–çŠ¶æ…‹ã‚’æ›´æ–°
            document.querySelectorAll('.ingredient-period').forEach(btn => {
                btn.classList.remove('active');
            });
            event.target.classList.add('active');

            // æˆåˆ†ã‚’å†è¡¨ç¤º
            displayIngredients();
        }

        // å®Ÿç”¨åŒ–æˆåˆ†ã‚’è¡¨ç¤º
        function displayIngredients() {
            const container = document.getElementById('ingredientGrid');
            container.innerHTML = '';

            const ingredients = dataStore.ingredientsByPeriod[currentIngredientPeriod];

            ingredients.forEach(ingredient => {
                const div = document.createElement('div');
                div.className = 'ingredient-card';

                // è©³ç´°ãƒ¢ãƒ¼ãƒ€ãƒ«ç”¨ã®ãƒ‡ãƒ¼ã‚¿ã‚’æ¢ã™
                const detailData = dataStore.ingredients.find(ing => ing.name === ingredient.name);
                if (detailData) {
                    div.onclick = () => showIngredientDetail(detailData);
                }

                div.innerHTML = `
                    <div class="ingredient-name">${ingredient.name}</div>
                    <div class="ingredient-score">${ingredient.evidence}ä»¶</div>
                    <div style="font-size: 0.8rem; opacity: 0.9; margin-top: 3px;">${ingredient.growth}</div>
                `;
                container.appendChild(div);
            });
        }

        // ãƒˆãƒ”ãƒƒã‚¯è©³ç´°ã‚’è¡¨ç¤º
        function showTopicDetail(topic) {
            document.getElementById('modalTitle').textContent = topic.name;
            const modal = document.getElementById('detailModal');
            modal.style.display = 'block';

            // ãƒ‡ãƒ•ã‚©ãƒ«ãƒˆã§æ¦‚è¦ã‚¿ãƒ–ã‚’è¡¨ç¤º
            switchTab('overview');

            // ã‚³ãƒ³ãƒ†ãƒ³ãƒ„ã‚’è¨­å®š
            window.currentDetail = {
                type: 'topic',
                data: topic
            };

            updateModalContent();
        }

        // æˆåˆ†è©³ç´°ã‚’è¡¨ç¤º
        function showIngredientDetail(ingredient) {
            document.getElementById('modalTitle').textContent = ingredient.name;
            const modal = document.getElementById('detailModal');
            modal.style.display = 'block';

            // ãƒ‡ãƒ•ã‚©ãƒ«ãƒˆã§æ¦‚è¦ã‚¿ãƒ–ã‚’è¡¨ç¤º
            switchTab('overview');

            // ã‚³ãƒ³ãƒ†ãƒ³ãƒ„ã‚’è¨­å®š
            window.currentDetail = {
                type: 'ingredient',
                data: ingredient
            };

            updateModalContent();
        }

        // ã‚¿ãƒ–åˆ‡ã‚Šæ›¿ãˆ
        function switchTab(tabName) {
            // ã‚¿ãƒ–ãƒœã‚¿ãƒ³ã®ã‚¢ã‚¯ãƒ†ã‚£ãƒ–çŠ¶æ…‹ã‚’æ›´æ–°
            document.querySelectorAll('.tab-button').forEach(btn => {
                btn.classList.remove('active');
            });
            event.target?.classList.add('active');

            // ã‚³ãƒ³ãƒ†ãƒ³ãƒ„ã‚’æ›´æ–°
            window.currentTab = tabName;
            updateModalContent();
        }

        // ãƒ¢ãƒ¼ãƒ€ãƒ«ã‚³ãƒ³ãƒ†ãƒ³ãƒ„ã‚’æ›´æ–°
        function updateModalContent() {
            const container = document.getElementById('modalContent');
            const detail = window.currentDetail;
            const tab = window.currentTab || 'overview';

            if (!detail) return;

            let content = '';

            if (detail.type === 'topic') {
                const topic = detail.data;

                switch(tab) {
                    case 'overview':
                        content = `
                            <div class="detail-section">
                                <h3>ğŸ“Š æ¦‚è¦</h3>
                                <p>${topic.summary}</p>
                            </div>

                            <div class="detail-section">
                                <h3>ğŸ”¬ ä¸»è¦ãªç™ºè¦‹</h3>
                                <ul>
                                    ${topic.keyFindings.map(finding => `<li>${finding}</li>`).join('')}
                                </ul>
                            </div>

                            <div class="detail-grid">
                                <div class="detail-item">
                                    <div class="detail-label">æˆé•·ç‡</div>
                                    <div class="detail-value">+${topic.growth}%</div>
                                </div>
                                <div class="detail-item">
                                    <div class="detail-label">é–¢é€£è«–æ–‡æ•°</div>
                                    <div class="detail-value">${topic.papers}ä»¶</div>
                                </div>
                                <div class="detail-item">
                                    <div class="detail-label">æ¨å¥¨ç”¨é‡</div>
                                    <div class="detail-value">${topic.dosage}</div>
                                </div>
                                <div class="detail-item">
                                    <div class="detail-label">å®‰å…¨æ€§</div>
                                    <div class="detail-value">${topic.safety}</div>
                                </div>
                            </div>
                        `;
                        break;

                    case 'research':
                        content = `
                            <div class="detail-section">
                                <h3>ğŸ“š æœ€æ–°ç ”ç©¶è«–æ–‡</h3>
                                <div class="research-papers">
                                    ${topic.recentPapers.map(paper => `
                                        <div class="paper-item">
                                            <div class="paper-title">${paper.title}</div>
                                            <div class="paper-info">${paper.journal} - ${paper.date}</div>
                                        </div>
                                    `).join('')}
                                </div>
                            </div>

                            <div class="detail-section">
                                <h3>ğŸ§ª ç ”ç©¶ãƒˆãƒ¬ãƒ³ãƒ‰</h3>
                                <div class="chart-container">
                                    <canvas id="researchTrendChart"></canvas>
                                </div>
                            </div>
                        `;
                        setTimeout(() => createResearchChart(topic), 100);
                        break;

                    case 'market':
                        content = `
                            <div class="detail-section">
                                <h3>ğŸ’° å¸‚å ´åˆ†æ</h3>
                                <div class="detail-item">
                                    <div class="detail-label">å¸‚å ´è¦æ¨¡</div>
                                    <div class="detail-value">${topic.marketSize}</div>
                                </div>
                            </div>

                            <div class="detail-section">
                                <h3>ğŸ¯ å¿œç”¨åˆ†é‡</h3>
                                <ul>
                                    ${topic.applications.map(app => `<li>${app}</li>`).join('')}
                                </ul>
                            </div>

                            <div class="insights-box">
                                <h3>ğŸ’¡ ãƒãƒ¼ã‚±ãƒ†ã‚£ãƒ³ã‚°ã‚¤ãƒ³ã‚µã‚¤ãƒˆ</h3>
                                <div class="insight-item">
                                    <span class="insight-icon">ğŸ“ˆ</span>
                                    <span>å‰å¹´æ¯”${Math.round(topic.growth * 0.7)}%ã®å¸‚å ´æˆé•·</span>
                                </div>
                                <div class="insight-item">
                                    <span class="insight-icon">ğŸ‘¥</span>
                                    <span>ä¸»è¦ã‚¿ãƒ¼ã‚²ãƒƒãƒˆï¼š30-50ä»£å¥³æ€§</span>
                                </div>
                                <div class="insight-item">
                                    <span class="insight-icon">ğŸª</span>
                                    <span>ECè²©å£²ãŒå…¨ä½“ã®65%ã‚’å ã‚ã‚‹</span>
                                </div>
                            </div>
                        `;
                        break;

                    case 'prediction':
                        content = `
                            <div class="detail-section">
                                <h3>ğŸ”® å°†æ¥äºˆæ¸¬</h3>
                                <div class="timeline-detail">
                                    <div class="timeline-event">
                                        <strong>3ãƒ¶æœˆå¾Œ</strong>
                                        <p>ç ”ç©¶è«–æ–‡æ•°ãŒ${topic.papers * 2}ä»¶ã«å¢—åŠ äºˆæ¸¬</p>
                                    </div>
                                    <div class="timeline-event">
                                        <strong>6ãƒ¶æœˆå¾Œ</strong>
                                        <p>æ–°è£½å“ãƒ­ãƒ¼ãƒ³ãƒãŒ${Math.round(topic.growth * 0.1)}ä»¶äºˆæƒ³</p>
                                    </div>
                                    <div class="timeline-event">
                                        <strong>1å¹´å¾Œ</strong>
                                        <p>å¸‚å ´è¦æ¨¡ãŒ1.5å€ã«æ‹¡å¤§è¦‹è¾¼ã¿</p>
                                    </div>
                                </div>
                            </div>

                            <div class="prediction-detail">
                                <h3>ğŸ“Š æˆé•·äºˆæ¸¬ã‚°ãƒ©ãƒ•</h3>
                                <div class="chart-container">
                                    <canvas id="predictionChart"></canvas>
                                </div>
                            </div>
                        `;
                        setTimeout(() => createPredictionChart(topic), 100);
                        break;
                }
            } else if (detail.type === 'ingredient') {
                const ingredient = detail.data;

                switch(tab) {
                    case 'overview':
                        content = `
                            <div class="detail-section">
                                <h3>ğŸ“Š åŸºæœ¬æƒ…å ±</h3>
                                <div class="detail-grid">
